  AddKeysToAgent yes
  ChallengeResponseAuthentication yes
  IdentityFile ~/.ssh/id_rsa
  HashKnownHosts yes
  ControlMaster auto
  ControlPath /tmp/%r@%h:%p
  ControlPersist 1
  Compression yes

